{
  "name": "pet-detective",
  "version": "1.0.0",
  "description": "AI-powered pet classification and gaming platform with deep learning models",
  "private": true,
  "scripts": {
    "flask-dev": "FLASK_DEBUG=1 pip3 install -r requirements.txt && python3 -m flask --app api/index run -p 5328",
    "next-dev": "next dev",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next out dist",
    "type-check": "tsc --noEmit",
    "delete-blobs": "npx ts-node scripts/delete-all-blobs.ts",
    "delete-blobs:dry-run": "npx ts-node scripts/delete-all-blobs.ts --dry-run",
    "delete-blobs:confirm": "npx ts-node scripts/delete-all-blobs.ts --confirm"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vercel/blob": "^1.1.1",
    "autoprefixer": "^10.4.16",
    "cloudinary": "^2.7.0",
    "next": "^14.0.4",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4"
  },
  "keywords": [
    "pet-classification",
    "deep-learning",
    "pytorch",
    "nextjs",
    "flask",
    "supabase",
    "ai-gaming",
    "image-segmentation"
  ],
  "author": "Pet Detective Team",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=18.17.0"
  }
}
